
public class printNreverseString {

	public static void main(String[] args) {
	
		String Name = "Abhijit";
		
		for (int i=0;i<Name.length();i++)
		{
			System.out.println("Print string");
			System.out.println(Name.charAt(i));
		}
		
		
		for (int i=Name.length();i<=0;i--)
		{
			System.out.println("Print string");
			System.out.println(Name.charAt(i));
		}
		

	}

}
