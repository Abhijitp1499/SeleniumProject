
public class printNreverseString {

	public static void main(String[] args) {
	
		String Name = "Abhijit";
		
		for (int i=0;i<Name.length();i++)
		{

			System.out.println(Name.charAt(i));
		}
		
		
		for (int j=Name.length()-1;j>=0;j--)
		{
		
			System.out.println(Name.charAt(j));
		}
		

	}

}
